extends layout

block content
    div(class='container')
        div(class='row')
            div(class='col s12 center')
                h5= title
            div(class='center')
                if loggedIn
                    a(class='waves-effect waves-light btn-large center', href='/logout') Logout
                else
                    a(class='waves-effect waves-light btn-large center', href='/login') Login with Azure Active Directory
